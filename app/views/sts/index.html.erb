<p id="notice"><%= notice %></p>

<h1>Sts</h1>

<table>
  <thead>
    <tr>
      <th>Number1</th>
      <th>Numder2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sts.each do |st| %>
      <tr>
        <td><%= st.number1 %></td>
        <td><%= st.numder2 %></td>
        <td><%= link_to 'Show', st %></td>
        <td><%= link_to 'Edit', edit_st_path(st) %></td>
        <td><%= link_to 'Destroy', st, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New St', new_st_path %>
